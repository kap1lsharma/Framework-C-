{
  // All fields are optional. Default will be considered if not provided.
  "Type": "browser", // [browser/mobile/desktop] Default is browser
  "BrowserType": "chrome", // [chrome/firefox/ie/safari]Default Browser Chrome [Default chrome driver support browser version 78]
  "MobileType": "android", // [android/ios] Default is android
  "MobileAppType": "", // [hybrid/native] Default is hybrid
  "BrowserVersion": "", // Blank
  "GridHubUrl": "", // default is local. Grid url format --https://<Grid-IPAddress>:4444/wd/hub
  "AppiumUrl": "", // default is local. Appium url format --https://<Appium-ServerAddress>:4523/wd/hub

  "Release": "OasisLoyalty_2.0", // Release

  "Region": "OasisLoyalty2_Halo", // Region name should match with Region XML file if provided.
  "RegionXmlFile": "", // Default is provided under automation runner.

  "StepScreenShot": "off", // [on/off] Default is off. To get screen shot for each step mention ON otherwise screen shot of failure will be added in extent report.
  "ExtentReportPortalUrl": "", // Default as mentioned. Recommended to have seperate report portal setup for each team.
  "ExtentReportToPortal": "off", // [on/off] Default is off. Report push to portal if mentioned ON.

  "LogLevel": "ALL", // [ALL/DEBUG/INFORMATION/WARN/ERROR/FATAL] Default:"INFORMATION. ALL < DEBUG < INFORMATION < WARN < ERROR < FATAL.
  "DataSetLocation": "", // Default is blank & consider under TestDataSet Folder. User can provide there location to upload data.

  "DownloadedLocation": "", // Default is blank & consider under Download Folder. User can provide there location to download data.
  "PerformanceData": "off", // Default is off. To collect performance data mention On.
  "LogLocation": "", // Default is blank & consider under Result Folder. User can provide the location to generate logs.
  "MobileInfo": "", // Blank is default. To run on mobile devices
  "Angular": "off" // Default is off.

}